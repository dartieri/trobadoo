@model trobadoo.com.web.Models.Contact.ContactModel
<style type="text/css">
    /*html { height: 100% }
  body { height: 100%; margin: 0px; padding: 0px }*/
    #map
    {
        float: left;
        width: 700px;
    }
    #map-canvas
    {
        height: 400px;
        width: 690px;
    }
</style>
<!-- Mapa -->
<div id="mapaContacto" style="width: 100%; height: 500px;">
</div>
<div id="contCentral" class="contCentralContacto">
    <div class="container clearfix">
        <div class="datosContacto mv-40">
            <div class="row">
                <!-- Dirección -->
                <div class="col-xs-12 col-sm-6 col-md-5 col-md-offset-1 col-lg-4 col-lg-offset-2 mb-15">
                    <h3 class="tituloDestacado">
                        @Model.GetTranslation("1")
                    </h3>
                    <p class="txtMedio mt-5">
                        <strong>TROBADOO SEGUNDA MANO SL</strong><br />
                        CIF: B57775876<br />
                        Calle Llucmajor, 152<br />
                        07007 Palma de Mallorca<br />
                        Es Molinar
                    </p>
                    <p class="txtMedio mt-5">
                        <strong>@Model.GetTranslation("2"):</strong> 971 250 740<br />
                        <strong>@Model.GetTranslation("3"):</strong> 629 475 583
                    </p>
                </div>
                <!-- Horarios -->
                <div class="col-xs-12 col-sm-6 col-md-5 col-lg-4">
                    <h3 class="tituloDestacado">
                        @Model.GetTranslation("4")
                    </h3>
                    <p class="txtMedio mt-5">
                        <strong>@Model.GetTranslation("5"):</strong> 10h-14h y 16h-20h<br />
                        <strong>@Model.GetTranslation("6"):</strong> 10h-14h
                    </p>
                    <p class="txtMedio mt-5">
                        @Model.GetTranslation("7") <a href="@Url.Action("Form", "Contact")" class="rojo underline">
                            @Model.GetTranslation("8")</a> @Model.GetTranslation("9") <a href="mailto:info@trobadoo.com"
                                class="rojo underline">info@trobadoo.com</a>.
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>